{
  "LambdaFunctionConfigurations": [
    {
      "Id": "H3TourProcessorTrigger",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:991497151548:function:H3TourProcessor",
      "Events": ["s3:ObjectCreated:*"],
      "Filter": {
        "Key": {
          "FilterRules": [
            {"Name": "prefix", "Value": "uploads/"},
            {"Name": "suffix", "Value": ".zip"}
          ]
        }
      }
    }
  ]
}